"""
Input: "Let's take LeetCode contest"
Output: "s'teL ekat edoCteeL tsetnoc"
"""

#85% 
class Solution:
    def reverseWords(self, s: str) -> str:
        words = s.split(" ")
        ans = []
        for i in words:
            ans.append(i[::-1])
        return " ".join(ans)
    
    
#split, join, reverse a string
